<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.berland.nathalie.placar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Placar"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.placar.PlacarActivity"></activity>
        <activity android:name=".ui.time.TimesActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.placar.placaraac.PlacarAACActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>